{
  "compilerOptions": {
    "baseUrl": "../../",
    "paths": {
      "common/*": ["common/*"],
      "@model": ["server/model/index"],
      "@constant": ["common/constant"],
      "@base/*": ["client/component/base/*"],
      "@container/*": ["client/component/container/*"]
    },
    "module": "commonjs",
    "target": "esnext",
    "sourceMap": true,
    "jsx": "react",
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
