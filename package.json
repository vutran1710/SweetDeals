{
  "name": "keothom",
  "version": "1.0.0",
  "description": "wt",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config -r ts-node/register --inspect fuse.ts",
    "start:prod": "DOTENV_CONFIG_PATH=.env.prod node -r dotenv/config -r ts-node/register fuse.ts",
    "lint": "tslint --fix -c tslint.json '{client,server}/**/*.{ts,tsx}'",
    "test": "DOTENV_CONFIG_PATH=.env.test mocha --opts test/mocha.opts",
    "cover": "DOTENV_CONFIG_PATH=.env.test nyc mocha --opts test/mocha.opts",
    "verify": "run-s lint test",
    "docker": "docker-compose -f docker-compose.dev.yaml up -d"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^4.3.3",
    "dotenv": "^8.0.0",
    "es6-template-strings": "^2.0.1",
    "express": "^4.17.1",
    "mongoose": "^5.5.15",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/ramda": "^0.26.9",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "fuse-box": "^3.7.1",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.17.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.6.0"
  },
  "nyc": {
    "include": [
      "server/**"
    ],
    "exclude": [
      "test/**"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
